          next if ELoot.data.ready_list.find { |k,v| v.id == thing.id}
          next if !ELoot.data.settings[:sell_exclude].empty? && thing.name =~ /#{ELoot.data.settings[:sell_exclude].join('|')}/
          next if thing.name =~ /bound/
          $sell_ignore.push(thing.id)          
