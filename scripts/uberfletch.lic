      check = matchtimeout 6, "With a few quick cuts,", "Working a bit too hastily, you slice the end right off!"
      if check == false
        echo "didnt see you cut the shaft for some reason, stopping"
      exit
      end
  end

  waitrt?
  stow("left",settings['fletch_sack'])


end

