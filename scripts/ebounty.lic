    def self.find_npc(npc_names)
      EBounty.msg("debug", " #{__method__} | caller: #{caller[0]}")

      readable = npc_names.source.gsub(/\(\?i-mx:/, "").gsub(/\)/, "")
      npc = nil

      (1..5).each do |attempt|
        npc = (GameObj.room_desc.to_a + GameObj.npcs.to_a).find { |t| t.name =~ npc_names }
        break if npc

        echo
        EBounty.msg "yellow", "Trying to detect NPC: #{readable} (attempt #{attempt})" if attempt > 1
        echo

        EBounty.wait_rt
      end

      unless npc
        echo
        EBounty.msg "yellow", "NPC not found after 5 attempts. This shouldn't happen."
        EBounty.msg "yellow", "Please report to EO."
        echo
        EBounty.exit_script
      end

      return npc
    end

