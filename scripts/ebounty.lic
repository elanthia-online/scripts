    def self.creature_culling(creature)
      EBounty.msg("debug", "creature_culling | creature: #{creature}")

      if (EBounty.data.settings[:creature_exclude].length.positive? && creature.downcase =~ Regexp.union(EBounty.data.settings[:creature_exclude]))
